        setTimeout(function () {
          /* istanbul ignore next */
          cov_5k0cn0s8y.f[9]++;
          cov_5k0cn0s8y.s[38]++;
          return listByIds(type, api);
        }, interval);
      } else
      /* istanbul ignore next */
      {
        cov_5k0cn0s8y.b[6][1]++;
      }
    }).then(setItem);
  }, [type, id]);

  /* istanbul ignore next */
  cov_5k0cn0s8y.s[39]++;
  return item;
};