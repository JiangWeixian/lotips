{
  "name": "@lotips/core",
  "version": "0.8.3",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./ua": {
      "require": "./dist/ua.cjs",
      "import": "./dist/ua.mjs",
      "types": "./dist/ua.d.ts"
    },
    "./do-highlight": {
      "require": "./dist/do-highlight.cjs",
      "import": "./dist/do-highlight.mjs",
      "types": "./dist/do-highlight.d.ts"
    },
    "./do-search": {
      "require": "./dist/do-search.cjs",
      "import": "./dist/do-search.mjs",
      "types": "./dist/do-search.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "ua": [
        "dist/ua.d.ts"
      ],
      "do-highlight": [
        "dist/do-highlight.d.ts"
      ],
      "do-search": [
        "dist/do-search.d.ts"
      ]
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c -w",
    "pretest": "pnpm run build",
    "test": "pnpm run pretest && vitest"
  },
  "dependencies": {
    "is-mobile": "^3.1.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/lodash-es": "^4.17.6",
    "rimraf": "3.0.2",
    "rollup": "2.70.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "tslib": "2.3.1",
    "typescript": "4.6.2",
    "vitest": "^0.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
