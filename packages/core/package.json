{
  "name": "@aiou/rollup-template",
  "version": "1.5.2",
  "main": "lib/how-long-till-lunch.cjs.js",
  "module": "lib/how-long-till-lunch.esm.js",
  "browser": "lib/how-long-till-lunch.umd.js",
  "typings": "lib/index.d.ts",
  "dependencies": {
    "ms": "2.1.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.22",
    "@types/ms": "0.7.31",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "np": "5.1.3",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "rimraf": "3.0.0",
    "rollup": "1.26.5",
    "rollup-plugin-alias": "2.2.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-typescript2": "0.25.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.5.0",
    "tslib": "1.10.0",
    "typescript": "3.7.2"
  },
  "scripts": {
    "build": "rimraf lib && rollup -c",
    "dev": "rollup -c -w",
    "test": "ts-node test/test.ts",
    "pretest": "npm run build",
    "update": "npm update && npm update --save-dev",
    "check": "npm outdated && npm outdated --save-dev",
    "prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish --any-branch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
